{% extends 'base.html.twig' %}


{% block title %}Création d'une tache
{% endblock %}

{% block body %}

	<h1>Création d'une tache</h1>

	<div class="form-wrapper">
		{{ form_start(form) }}

		{{ form_row(form.lieu, { 'label': false }) }}
		{{ form_widget(form.heureDebut) }}
		{{ form_widget(form.heureFin) }}
		{{ form_widget(form.creerTache) }}

		{{ form_end(form) }}
	</div>


{% endblock %}
